{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/Image/*",
      "use": "@vercel/static"
    },
    {
      "src": "assets/Songs/*",
      "use": "@vercel/static"
    },
    {
      "src": "CSS/responsive.css",
      "use": "@vercel/static"
    },
    {
      "src": "CSS/style.css",
      "use": "@vercel/static"
    },
    {
      "src": "JS/script.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
